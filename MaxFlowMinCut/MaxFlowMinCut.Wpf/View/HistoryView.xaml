<Window x:Class="MaxFlowMinCut.Wpf.View.HistoryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:graphViewerGdi="clr-namespace:Microsoft.Msagl.GraphViewerGdi;assembly=Microsoft.Msagl.GraphViewerGdi"
        xmlns:visualizer="clr-namespace:MaxFlowMinCut.Wpf.Visualizer"
        xmlns:his="clr-namespace:MaxFlowMinCut.Lib.History;assembly=MaxFlowMinCut.Lib"
        xmlns:userControl="clr-namespace:MaxFlowMinCut.Wpf.UserControl"
        AllowsTransparency="False"
        Title="HistoryView" Height="600" Width="700">
    <Grid>
        <userControl:DependencyGraphWrapper Grid.Column="0" FlowGraph="{Binding FlowGraph}" Height="300"
                                            HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />

    </Grid>
</Window>